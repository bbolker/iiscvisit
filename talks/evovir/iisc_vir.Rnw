## sources ("Turell altjmh","Dohm","Turell_JME") and get $n$
## values if possible).
virdat <- mutate(virdat,
                 n_inf=round(inf/100*n),
                 n_transmission=round(transmission/100*n),
                 inf=100*n_inf/n,
                 transmission=ifelse(is.na(n),transmission,
                 100*n_transmission/n))
mprof <- with(virdat,seq(max(transmission),100,length=25))/100
glm0 <- glm(transmission/100~titer,weight=n,
            data=virdat,
            family=binomial)
mfit <- function(m) {
    glm(transmission/100~titer,weight=n,
        data=virdat,
        family=binomial(link=logit.2asym(0,1-m)),
        start=coef(glm0))
}
