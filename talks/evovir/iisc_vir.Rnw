pfun <- function(titer,min=0,max=1) {
  pmin(max,pmax(0.293*titer-1.48,min))
}

Tfun <- function(cm) {
  334.7-70.3*log(cm*100)
}
tot.trans <- function(cm) {
  Tval <- Tfun(cm)
  a <- predict(lm.a,newdata=list(cm0=cm))
  b <- predict(lm.b,newdata=list(cm0=cm))
  day <- 0:(round(Tval)-1)
  titer <- a*day*exp(-b*day)
  transm <- pfun(titer)
  tot.transm <- sum(transm)
  c(totbeta=tot.transm,T=Tval,avgbeta=tot.transm/Tval,a=a,b=b)
}
cmvec <- seq(0.01,0.99,by=0.01)
vals <- t(sapply(cmvec,tot.trans))
z <- splinefun(1/vals[,"T"],vals[,"avgbeta"])
## calculate intersection/equivalent curve
m <- 0.006
u1 <- uniroot(function(x) z(x,1)-z(x)/(x+m),interval=c(0.01,0.04))
## zero-mortality case
u2 <- uniroot(function(x) z(x,1)-z(x)/(x),interval=c(0.01,0.04))
## abline(v=u1$root)
## abline(v=u2$root,lty=2)
pos <- u1$root
val <- z(u1$root)
sl <- z(u1$root,1)
gam <- val/(pos*sl)
c0 <- val*pos^(-1/gam)
@

\section[Seasonal disease]{Transient virulence and seasonality}

\subsection{Overview}
\begin{frame}
\frametitle{Seasonality}
\begin{itemize}
\item{Many pathogens fluctuate annually
  \begin{itemize}
  \item{Host contact/aggregation patterns}
  \item{Host (or vector) demography}
  \item{Climatic effects on transmissibility}
  \end{itemize}
}
\item{Fluctuating incidence = fluctuating selection}
\item{Seasonal variation \emph{or} latitudinal variation?}
\end{itemize}
\end{frame}

